{
  "name": "vue3-usecookies",
  "private": false,
  "version": "1.0.4",
  "type": "module",
  "scripts": {
    "playground": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "pub": "npm run build && npm publish"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VashUber/vue3-usecookies.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "cookie",
    "hook"
  ],
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.4.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.5"
  },
  "peerDependencies": {
    "vue": "^3.2.37"
  }
}
